    "require": {
        "firebase/php-jwt": "*",
        "vlucas/phpdotenv": "*",
        "ext-json": "*",
        "ext-pdo": "*",
	"symfony/http-foundation": "*"

    },
    "require-dev": {
	"squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
